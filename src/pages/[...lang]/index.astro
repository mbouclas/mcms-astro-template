---
import MainLayout from '../../layouts/main.astro';
import HomepageWraper from '../../components/homepage/container.astro';
import {Data} from '../../services/boot';


await Data.boot();
export async function getStaticPaths() {

const paths = Data.locales.map(({ astro, url, file, ...locale }) => {
    return {
      params: {
        lang: locale.code
      },
      props: {
        lang: locale.code
      }
    };
});

// return paths.filter(({ params }) => !!params.lang);
return paths;
}


const {lang} = Astro.props;
---

<MainLayout>
    <HomepageWraper></HomepageWraper>
</MainLayout>