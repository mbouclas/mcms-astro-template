---
import MainLayout from '../../layouts/main.astro';
import {Data} from '../../services/boot';
import HomepageWraper from '../../components/homepage/container.astro';

await Data.boot();
export async function getStaticPaths() {

const paths = Data.locales.map(({ astro, url, file, ...locale }) => {
    return {
      params: {
        lang: locale.code
      },
      props: {
        lang: locale.code
      }
    };
});

// return paths.filter(({ params }) => !!params.lang);
return paths;
}


const {lang} = Astro.props;

---

  <MainLayout>
<HomepageWraper lang={lang}></HomepageWraper>
</MainLayout>